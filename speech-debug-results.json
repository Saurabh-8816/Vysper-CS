{
  "environment": {
    "nodeVersion": "v22.13.1",
    "platform": "win32",
    "envFile": true
  },
  "dependencies": {
    "microsoft-cognitiveservices-speech-sdk": true,
    "node-record-lpcm16": true,
    "dotenv": true,
    "sox": true
  },
  "speechService": {
    "status": {
      "isRecording": false,
      "isInitialized": true,
      "sessionDuration": 0,
